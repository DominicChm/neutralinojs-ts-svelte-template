{
  "applicationId": "typescript-svelte-vite-template",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 8080,
  "documentRoot": "/build/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "globalVariables": {
    "ExampleGlobal": "defined in neutralino.config.json"
  },
  "modes": {
    "window": {
      "title": "Neutralino Template! (Change me in neutralino.config.json)!",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/assets/logo.png",
      "enableInspector": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "exitProcessOnClose": false,
      "resizable": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "CHANGEME",
    "clientLibrary": "/public/neutralino.js",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}